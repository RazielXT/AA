
void main_vs(uniform float4x4 wmat,
    in float4 inPos : POSITION,
    out float4 pos : POSITION
	)
{
    pos = mul(wmat, inPos);
}


void main_ps(
		out float4 color0 : COLOR0,
   		out float4 color1 : COLOR1,
 		out float4 color2 : COLOR2,
 		out float4 color3 : COLOR3
		)
{
	color0=float4(1.0,1.0,1.0,1.0);
	color1=float4(0.5,0.5,1.0,1);
	color2=color0;
	color3=float4(1,64000,0,1);
}