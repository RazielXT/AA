material terrainRock : basic_2tex_vc
{
	set $uvScale "2"
	set $uv2Scale "2"

	set_texture_alias diffuseMap rock2.png
	set_texture_alias normalMap rock2_NORM.tga
	set_texture_alias dispMap rock2_DISP.png
	
	set_texture_alias diffuseMap2 SilveryBlue.jpg
	set_texture_alias normalMap2 SilveryBlue_NORM.tga
	
	set_texture_alias normalDetailMap rock3_NORM_NORM.tga
	set_texture_alias spreadMap spread.png

	set $diffuseCol "1 1 1"
	set $specularCol "1 1 1"
	set $shininess "64"
}

material oldWallMat : basic_2tex_vc
{
	set $uvScale "1"
	set $uv2Scale "1"

	set_texture_alias diffuseMap brick19.jpg
	set_texture_alias normalMap brick19_NORM.tga
	set_texture_alias dispMap brick19_DISP.jpg

	set_texture_alias diffuseMap2 dejavu.png
	//set_texture_alias normalMap2 rock2_NORM.tga
	
	set_texture_alias normalDetailMap rock2_NORM.tga
	set_texture_alias spreadMap brick19_DISP.jpg

	set $diffuseCol "1 1 1"
	set $specularCol "1 1 1"
	set $shininess "64"
}

material oldWoodFloor : basic_2tex_vc
{
	set $uvScale "2"
	set $uv2Scale "2"

	set_texture_alias diffuseMap wood0.png
	set_texture_alias normalMap wood0_NORM.tga
	set_texture_alias dispMap wood0_DISP.png

	set_texture_alias diffuseMap2 rock2.png
	set_texture_alias normalMap2 rock2_NORM.tga
	
	set_texture_alias normalDetailMap rock2_NORM.tga
	set_texture_alias spreadMap spread.png

	set $diffuseCol "1 1 1"
	set $specularCol "1 1 1"
	set $shininess "64"
}