fragment_program PutThrough_fp cg
{
	source PostProcThroughs.cg
	profiles ps_2_x

	entry_point PutThrough_fp
	
}

material PutThrough
{
	technique
	{
		pass
		{
			cull_hardware none
			cull_software none
			depth_func always_pass

			fragment_program_ref PutThrough_fp
			{
			}

			texture_unit map
			{
				tex_coord_set 0
				tex_address_mode clamp
				filtering bilinear
			}
		}
	}
}

