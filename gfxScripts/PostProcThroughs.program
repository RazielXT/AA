fragment_program CombineScene_fp cg
{
	source PostProcThroughs.cg
	profiles ps_2_x

	entry_point CombineScene_fp
	
	default_params
    {
		param_named_auto time time
    }
}

material CombineScene
{
	technique
	{
		pass
		{
			cull_hardware none
			cull_software none
			depth_func always_pass

			fragment_program_ref CombineScene_fp
			{
			}

			texture_unit scene
			{
					tex_coord_set 0
					tex_address_mode clamp
					filtering none
			}

			texture_unit sceneDepth
			{
					tex_coord_set 0
					tex_address_mode clamp
					filtering none
			}
			
			texture_unit smoke
			{
					tex_coord_set 0
					tex_address_mode clamp
					filtering anisotropic
					max_anisotropy 8
			}

			texture_unit sceneTransparent
			{
					tex_coord_set 0
					tex_address_mode clamp
					filtering none
			}
			
			texture_unit sceneDistortion
			{
					tex_coord_set 0
					tex_address_mode clamp
					filtering none
			}
			
			texture_unit ppDistortion
			{
					filtering trilinear	
					texture underwater_n.tga
			}
        }
    }
}